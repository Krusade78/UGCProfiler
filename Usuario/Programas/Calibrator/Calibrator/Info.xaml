                    </Viewbox>
                    <TextBlock x:Name="Labelmxy" Text="X: 00&#x0a;Y: 00" Foreground="Yellow" Margin="10,0"  HorizontalAlignment="Left"  VerticalAlignment="Center"/>
                </StackPanel>
            </Grid>
        </Grid>

