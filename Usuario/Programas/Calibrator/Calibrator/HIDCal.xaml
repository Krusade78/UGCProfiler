<UserControl x:Class="Calibrator.HIDCal"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" MaxWidth="525" Height="630">
    <Grid>
        <Grid.Background>
            <LinearGradientBrush EndPoint="1,1" StartPoint="0,0">
                <GradientStop Color="#B2000000" Offset="0"/>
                <GradientStop Color="#A2000000" Offset="1"/>
            </LinearGradientBrush>
        </Grid.Background>
    </Grid>

</UserControl>
